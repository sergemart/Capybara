<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_initial_common"
    app:startDestination="@id/fragment_initial_common_setup">

    <fragment
        android:id="@+id/fragment_initial_common_setup"
        android:name="com.github.sergemart.mobile.capybara.controller.fragment.InitialCommonSetupFragment"
        android:label="fragment_initial_common_setup"
        tools:layout="@layout/fragment_initial_common_setup" >
        <action
            android:id="@+id/action_initialSetup_to_initialSignin"
            app:destination="@id/fragment_initial_common_sign_in" />
    </fragment>

    <fragment
        android:id="@+id/fragment_initial_common_sign_in"
        android:name="com.github.sergemart.mobile.capybara.controller.fragment.InitialCommonSignInFragment"
        android:label="fragment_initial_common_sign_in"
        tools:layout="@layout/fragment_initial_common_sign_in" />

    <fragment
        android:id="@+id/fragment_initial_common_upgrade_backend"
        android:name="com.github.sergemart.mobile.capybara.controller.fragment.InitialCommonUpgradeBackendFragment"
        android:label="fragment_initial_common_upgrade_backend"
        tools:layout="@layout/fragment_initial_common_upgrade_backend" />

</navigation>